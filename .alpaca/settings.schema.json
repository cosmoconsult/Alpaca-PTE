                                    "version": {
                                        "type": "string"
                                    },
                                    "url": {
                                        "type": "string"
                                    },
                                    "ignoreIn": {
                                        "type": "array",
                                        "items": {
                                            "enum": [
                                                "always",
                                                "development",
                                                "build"
                                            ]
                                        }
                                    }
                                },
                                "additionalProperties": false
                            }
                        }
                    }
                }
